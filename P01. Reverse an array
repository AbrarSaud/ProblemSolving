import java.util.Scanner;


public class MyClass {

	public static void main(String[] args) {
	    Scanner myObj = new Scanner(System.in);
	 
	 int x =myObj.nextInt();
           int [] arr1 =new int [x];
			for(int i =0; i<x ;i++) {
				
				arr1[i] =myObj.nextInt();
				
				
			}
	
			 int [] arr2 =new int [x];
				for(int i = x -1; i >= 0 ;i --) {
					
			        System.out.print(arr1[i] + " ");
					
					
				}	
}}
	     
