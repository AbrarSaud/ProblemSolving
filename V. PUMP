import java.util.*;
 
public class SaudiCPC {
 
	public static void main(String[] args) {
	
 
		Scanner myObj = new Scanner(System.in);
	    //String word = myObj.next();
	  int  num =myObj.nextInt();
	  int c = 1; 	  
		for(int i =1; i <= num ; i++) {
		    
			if(c % 4 == 0) {	
		        System.out.println(" PUM");
	             c ++;
	             i --;
			}else {
		        System.out.print(c++ +" "+ c++ +" "+ c++ );
	 
			}
		}
		 System.out.println(" PUM");
		
		
		
		
		
 
	}
 
}
