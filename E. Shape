import java.util.Scanner;


public class MyClass {

	public static void main(String[] args) {
	    Scanner myObj = new Scanner(System.in);
	  // String value = myObj.next();  
	   // int r = myObj.nextInt();
	    int n;
	    n = myObj.nextInt();
	    
        for(int i=1;i<=n;i++)
        { int k=1;
        for(int j=1;j<=i;j++) {
         System.out.print(k);
         k++;}
        k=1;
       System.out.println("");
       }
	               
				}

			
		
		}	
		 
